{
    "version": 2,
    "builds": [
      {
        "src": "dist/client/**",
        "use": "@vercel/static"
      },
      {
        "src": "dist/api/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "dist/api/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  