{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    },
    {
      "src": "client",
      "use": "@vercel/static",
      "dest": "client/dist/public"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/client/dist/public/index.html"
    }
  ]
}